<h1>Listing tipo_vacinas</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @tipo_vacinas.each do |tipo_vacina| %>
      <tr>
        <td><%= tipo_vacina.descricao %></td>
        <td><%= link_to 'Show', tipo_vacina %></td>
        <td><%= link_to 'Edit', edit_tipo_vacina_path(tipo_vacina) %></td>
        <td><%= link_to 'Destroy', tipo_vacina, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tipo vacina', new_tipo_vacina_path %>
