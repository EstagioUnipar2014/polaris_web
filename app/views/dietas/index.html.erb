<h1>Listing dietas</h1>

<table>
  <thead>
    <tr>
      <th>Descricao</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @dietas.each do |dieta| %>
      <tr>
        <td><%= dieta.descricao %></td>
        <td><%= link_to 'Show', dieta %></td>
        <td><%= link_to 'Edit', edit_dieta_path(dieta) %></td>
        <td><%= link_to 'Destroy', dieta, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Dieta', new_dieta_path %>
