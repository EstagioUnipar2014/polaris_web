<div class="panel panel-default">
	<div class="panel-heading">
		<span class="page-title animais">Tipo Exames</span>
		<%= link_to 'Novo Tipo exame', new_tipo_exame_path,  class: 'novo-animal-link' %>
		<table class="table table-condensed">
			<thead>
				<tr>
					<th>Descricao</th>
					<th colspan="3"></th>
				</tr>
			</thead>

			<tbody>
				<% @tipo_exames.each do |tipo_exame| %>
					<tr>
						<td><%= tipo_exame.descricao %></td>
						<td><%= link_to 'Show', tipo_exame %></td>
						<td><%= link_to 'Edit', edit_tipo_exame_path(tipo_exame) %></td>
						<td><%= link_to 'Destroy', tipo_exame, method: :delete, data: { confirm: 'Are you sure?' } %></td>
					</tr>
				<% end %>
			</tbody>
		</table>
	</div>
</div>